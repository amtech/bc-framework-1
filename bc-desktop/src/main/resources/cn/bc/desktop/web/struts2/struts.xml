<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="personalConfig" extends="bc-default" namespace="/bc/desktop/personalConfig">
		<action name="create" class="personalConfigAction" method="create">
			<result name="form">/bc/desktop/personalConfig/form.jsp</result>
		</action>
		<action name="edit" class="personalConfigAction" method="edit">
			<result name="form">/bc/desktop/personalConfig/form.jsp</result>
		</action>
				
		<action name="save" class="personalConfigAction" method="save"/>
		<action name="delete" class="personalConfigAction" method="delete"/>
		<action name="list" class="personalConfigAction" method="list"/>
		<action name="update" class="personalConfigAction" method="update"/>
	</package>
</struts>